<keymap version="1" name="Bob's Keymapping" parent="Default for XWin">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="alt 1" />
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl b" />
  </action>
  <action id="CheckinProject" />
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="Console.TableResult.NavigateExportedAction">
    <keyboard-shortcut first-keystroke="alt f7" />
  </action>
  <action id="Diff.NextChange">
    <keyboard-shortcut first-keystroke="alt right" />
  </action>
  <action id="Diff.SelectedChange" />
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorCodeBlockEnd" />
  <action id="EditorCodeBlockStart" />
  <action id="EditorDeleteLine" />
  <action id="EditorIndentSelection">
    <keyboard-shortcut first-keystroke="tab" />
    <keyboard-shortcut first-keystroke="ctrl close_bracket" />
  </action>
  <action id="EditorMoveToPageBottom" />
  <action id="EditorMoveToPageTop" />
  <action id="EditorSelectWord" />
  <action id="EditorUnindentSelection">
    <keyboard-shortcut first-keystroke="shift tab" />
    <keyboard-shortcut first-keystroke="ctrl open_bracket" />
  </action>
  <action id="Exit">
    <keyboard-shortcut first-keystroke="ctrl q" />
  </action>
  <action id="FileChooser.NewFolder">
    <keyboard-shortcut first-keystroke="alt insert" />
  </action>
  <action id="FindNext">
    <keyboard-shortcut first-keystroke="f3" />
  </action>
  <action id="GotoChangedFile" />
  <action id="GotoClass">
    <keyboard-shortcut first-keystroke="ctrl t" />
  </action>
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
    <keyboard-shortcut first-keystroke="shift ctrl t" />
  </action>
  <action id="GotoLine">
    <keyboard-shortcut first-keystroke="ctrl g" />
    <keyboard-shortcut first-keystroke="ctrl l" />
  </action>
  <action id="GotoSymbol">
    <keyboard-shortcut first-keystroke="shift ctrl alt n" />
    <keyboard-shortcut first-keystroke="shift ctrl alt t" />
  </action>
  <action id="GotoTest" />
  <action id="Images.ShowThumbnails" />
  <action id="NewElement">
    <keyboard-shortcut first-keystroke="alt insert" />
    <keyboard-shortcut first-keystroke="ctrl n" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="ctrl page_down" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="ctrl page_up" />
  </action>
  <action id="QuickJavaDoc">
    <mouse-shortcut keystroke="alt button2" />
  </action>
  <action id="Refactorings.QuickListPopupAction" />
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="shift alt 2" />
  </action>
  <action id="UnsplitAll">
    <keyboard-shortcut first-keystroke="shift alt 1" />
  </action>
  <action id="Vcs.Log.FocusTextFilter" />
  <action id="Vcs.UpdateProject" />
</keymap>